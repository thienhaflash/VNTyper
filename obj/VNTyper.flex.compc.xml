<?xml version="1.0"?>
<flex-config xmlns="http://www.adobe.com/2006/flex-config">
  <output>D:\github\VNTyper\.\bin\VNTyper.swc</output>
  <use-network>true</use-network>
  <target-player>9</target-player>
  <warnings>true</warnings>
  <benchmark>false</benchmark>
  <compiler>
    <accessible>false</accessible>
    <allow-source-path-overlap>false</allow-source-path-overlap>
    <optimize>true</optimize>
    <strict>true</strict>
    <es>false</es>
    <show-actionscript-warnings>true</show-actionscript-warnings>
    <show-binding-warnings>true</show-binding-warnings>
    <show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>
    <use-resource-bundle-metadata>true</use-resource-bundle-metadata>
    <verbose-stacktraces>false</verbose-stacktraces>
    <source-path>
      <path-element>d:\github\vntyper\lib</path-element>
      <path-element>d:\github\vntyper\src</path-element>
    </source-path>
  </compiler>
  <include-classes>
    <class>vn.typer.VNTyper</class>
    <class>vn.typer.core.Char</class>
    <class>vn.typer.core.Consonant</class>
    <class>vn.typer.core.Encoder</class>
    <class>vn.typer.core.TypeMode</class>
    <class>vn.typer.core.VNTyperCore</class>
    <class>vn.typer.core.Vowel</class>
    <class>vn.typer.core.Word</class>
    <class>vn.typer.encode.VniEncoder</class>
    <class>vn.typer.modes.TelexMode</class>
    <class>vn.typer.modes.TelexVniMode</class>
    <class>vn.typer.modes.VniMode</class>
    <!--<class>VNTyperTest</class>-->
  </include-classes>
</flex-config>